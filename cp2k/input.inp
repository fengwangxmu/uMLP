&GLOBAL
        PROJECT labeling
&END GLOBAL
&FORCE_EVAL
        METHOD QS
        STRESS_TENSOR ANALYTICAL
        &DFT
                BASIS_SET_FILE_NAME BASIS_MOLOPT
                POTENTIAL_FILE_NAME GTH_POTENTIALS
                CHARGE 0
                UKS F
                MULTIPLICITY 1
                &MGRID
                        CUTOFF 1600
                        REL_CUTOFF 90
                        NGRIDS 4
                &END MGRID
                &QS
                        EPS_DEFAULT 1e-13
                        EXTRAPOLATION ASPC
                        EXTRAPOLATION_ORDER 2
                &END QS
                &SCF
                        SCF_GUESS RESTART
                        EPS_SCF 1e-06
                        MAX_SCF 20
                        &OUTER_SCF
                                EPS_SCF 1e-06
                                MAX_SCF 50
                        &END OUTER_SCF
                        &OT
                                MINIMIZER CG
                                PRECONDITIONER FULL_SINGLE_INVERSE
                                #ENERGY_GAP 0.1
                        &END OT
                        &PRINT 
                                &RESTART  OFF
                                &END RESTART 
                        &END PRINT 
                &END SCF
                &XC
                        &XC_FUNCTIONAL PBE
                        &END XC_FUNCTIONAL
                        &VDW_POTENTIAL
                                DISPERSION_FUNCTIONAL PAIR_POTENTIAL
                                &PAIR_POTENTIAL
                                        TYPE DFTD3
                                        PARAMETER_FILE_NAME dftd3.dat
                                        REFERENCE_FUNCTIONAL PBE
                                &END PAIR_POTENTIAL
                        &END VDW_POTENTIAL
                &END XC
        &END DFT
        &SUBSYS
@include coord_n_cell.inc
                &KIND Si
                        BASIS_SET DZVP-MOLOPT-SR-GTH
                        POTENTIAL GTH-PBE-q4
                &END KIND
                &KIND I
                        BASIS_SET DZVP-MOLOPT-SR-GTH
                        POTENTIAL GTH-PBE-q7
                &END KIND
                &KIND Br
